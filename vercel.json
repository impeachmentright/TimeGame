{
    "version": 2,
    "builds": [
        {
            "src": "server.mjs",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/web-data",
            "dest": "/server.mjs"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ]
}